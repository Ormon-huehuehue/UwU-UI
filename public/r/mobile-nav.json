{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "mobile-nav",
  "type": "registry:ui",
  "dependencies": [],
  "author": "daniel petho <https://www.danielpetho.com>",
  "registryDependencies": [
    "https://uwuui.com/r/utils.json",
    "button",
    "scroll-area",
    "sheet"
  ],
  "files": [
    {
      "path": "components/mobile-nav.tsx",
      "content": "\"use client\"\n\nimport * as React from \"react\"\nimport Link, { LinkProps } from \"next/link\"\nimport { useRouter } from \"next/navigation\"\n\nimport { docsConfig } from \"@/config/docs\"\nimport { cn } from \"@/lib/utils\"\nimport { Button } from \"@/components/ui/button\"\nimport { ScrollArea } from \"@/components/ui/scroll-area\"\nimport { Sheet, SheetContent, SheetTrigger } from \"@/components/ui/sheet\"\n\nexport function MobileNav() {\n  const [open, setOpen] = React.useState(false)\n\n  return (\n    <Sheet open={open} onOpenChange={setOpen}>\n      <SheetTrigger asChild>\n        <Button\n          variant=\"ghost\"\n          className=\"mr-2 px-0 text-base hover:bg-transparent focus-visible:bg-transparent focus-visible:ring-0 focus-visible:ring-offset-0 lg:hidden group\"\n        >\n          <svg\n            strokeWidth=\"1.5\"\n            viewBox=\"0 0 24 24\"\n            fill=\"none\"\n            xmlns=\"http://www.w3.org/2000/svg\"\n            className=\"h-5 w-5\"\n          >\n            <path\n              d=\"M3 5H11\"\n              stroke=\"currentColor\" \n              strokeWidth=\"1.5\"\n              strokeLinecap=\"round\"\n              strokeLinejoin=\"round\"\n              className=\"group-hover:[stroke-width:3] transition-all duration-300 ease-out\"\n            ></path>\n            <path\n              d=\"M3 12H16\"\n              stroke=\"currentColor\"\n              strokeWidth=\"1.5\"\n              strokeLinecap=\"round\"\n              strokeLinejoin=\"round\"\n              className=\"group-hover:[stroke-width:3] transition-all duration-300 delay-100 ease-out\"\n            ></path>\n            <path\n              d=\"M3 19H21\"\n              stroke=\"currentColor\"\n              strokeWidth=\"1.5\"\n              strokeLinecap=\"round\"\n              strokeLinejoin=\"round\"\n              className=\"group-hover:[stroke-width:3] transition-all duration-300 delay-200 ease-out\"\n            ></path>\n          </svg>\n          <span className=\"sr-only\">Toggle Menu</span>\n        </Button>\n      </SheetTrigger>\n      <SheetContent side=\"left\" className=\"pr-0 m-4 rounded-2xl h-[calc(100vh-3rem)] w-4/5\">\n        <MobileLink\n          href=\"/\"\n          className=\"flex items-center\"\n          onOpenChange={setOpen}\n        >\n          {/* <Icons.logo className=\"mr-2 h-4 w-4\" /> */}\n          <p className=\" text-2xl tracking-tight font-calendas scale-y-[120%] \">\n            UwU UI\n          </p>\n        </MobileLink>\n        <ScrollArea className=\"my-8 h-[calc(100vh-8rem)] pb-24 pl-1\">\n          <div className=\"flex flex-col space-y-2\">\n            {docsConfig.map((item, index) => (\n              <div key={index} className=\"flex flex-col space-y-2 pt-6\">\n                <h4 className=\"text-2xl font-medium \">\n                  {item.title}{\" \"}\n                  <span className=\"align-super text-sm\">\n                    {item.title !== \"Getting Started\"\n                      ? `(${item.items?.length})`\n                      : \"\"}\n                  </span>\n                </h4>\n                {item?.items?.length &&\n                  item.items.map((item) => (\n                    <React.Fragment key={item.href}>\n                      {!item.disabled &&\n                        (item.href ? (\n                          <MobileLink\n                            href={item.href}\n                            onOpenChange={setOpen}\n                            className=\"text-base text-foreground/50\"\n                          >\n                            {item.title}\n                            {item.label && (\n                              <span className=\"ml-2 rounded-md bg-primary-blue px-1.5 py-0.5 text-xs leading-none text-white no-underline group-hover:no-underline\">\n                                {item.label}\n                              </span>\n                            )}\n                          </MobileLink>\n                        ) : (\n                          item.title\n                        ))}\n                    </React.Fragment>\n                  ))}\n              </div>\n            ))}\n          </div>\n        </ScrollArea>\n      </SheetContent>\n    </Sheet>\n  )\n}\n\ninterface MobileLinkProps extends LinkProps {\n  onOpenChange?: (open: boolean) => void\n  children: React.ReactNode\n  className?: string\n}\n\nfunction MobileLink({\n  href,\n  onOpenChange,\n  className,\n  children,\n  ...props\n}: MobileLinkProps) {\n  const router = useRouter()\n  return (\n    <Link\n      href={href}\n      onClick={() => {\n        router.push(href.toString())\n        onOpenChange?.(false)\n      }}\n      className={cn(className)}\n      {...props}\n    >\n      {children}\n    </Link>\n  )\n}\n",
      "type": "registry:ui",
      "target": "components/fancy/components/mobile-nav.tsx"
    }
  ]
}